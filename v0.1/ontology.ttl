                rdfs:subClassOf :Movement ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :involves ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :Finger
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :involves ;
                                  owl:maxQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                  owl:onClass :Finger
                                ] .


