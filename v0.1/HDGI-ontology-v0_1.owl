@prefix : <https://w3id.org/hdgi#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix hdgi: <https://w3id.org/hdgi#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <https://w3id.org/hdgi#> .

<https://w3id.org/hdgi#> rdf:type owl:Ontology ;
                          owl:versionIRI <https://w3id.org/hdgi/v0.1> .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/hdgi#affordedBy
hdgi:affordedBy rdf:type owl:ObjectProperty ;
                rdfs:comment "Relation from a Affordance to a Device describing the affordances of a device." ;
                rdfs:label "affordedBy" .


###  https://w3id.org/hdgi#hasContext
hdgi:hasContext rdf:type owl:ObjectProperty ;
                rdfs:label "Relation from a Human to a User Context or from a Device to a Device Context." ,
                           "hasContext" .


###  https://w3id.org/hdgi#hasDuration
hdgi:hasDuration rdf:type owl:ObjectProperty ;
                 rdfs:label "hasDuration" .


###  https://w3id.org/hdgi#hasEndPose
hdgi:hasEndPose rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf hdgi:hasPose ;
                rdfs:label "hasEndPose" .


###  https://w3id.org/hdgi#hasLocalCoordinateSystem
hdgi:hasLocalCoordinateSystem rdf:type owl:ObjectProperty ;
                              rdfs:label "hasLocalCoordinateSystem" .


###  https://w3id.org/hdgi#hasMovement
hdgi:hasMovement rdf:type owl:ObjectProperty ;
                 rdfs:label "hasMovement" .


###  https://w3id.org/hdgi#hasPart
hdgi:hasPart rdf:type owl:ObjectProperty ;
             owl:inverseOf hdgi:isPartOf ;
             rdfs:label "hasPart" .


###  https://w3id.org/hdgi#hasPose
hdgi:hasPose rdf:type owl:ObjectProperty ;
             rdfs:label "hasPose" .


###  https://w3id.org/hdgi#hasPosition
hdgi:hasPosition rdf:type owl:ObjectProperty ;
                 rdfs:label "hasPosition" .


###  https://w3id.org/hdgi#hasQuaternion
hdgi:hasQuaternion rdf:type owl:ObjectProperty ;
                   rdfs:label "hasQuaternion" .


###  https://w3id.org/hdgi#hasRotation
hdgi:hasRotation rdf:type owl:ObjectProperty ;
                 rdfs:label "hasRotation" .


###  https://w3id.org/hdgi#hasStartPose
hdgi:hasStartPose rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf hdgi:hasPose ;
                  rdfs:label "hasStartPose" .


###  https://w3id.org/hdgi#hasTimeStamp
hdgi:hasTimeStamp rdf:type owl:ObjectProperty ;
                  rdfs:label "hasTimeStamp" .


###  https://w3id.org/hdgi#hasUnitOfMeasure
hdgi:hasUnitOfMeasure rdf:type owl:ObjectProperty ;
                      rdfs:label "hasUnitOfMeasure" .


###  https://w3id.org/hdgi#includesGesture
hdgi:includesGesture rdf:type owl:ObjectProperty ;
                     rdfs:label "includesGesture" .


###  https://w3id.org/hdgi#involves
hdgi:involves rdf:type owl:ObjectProperty ;
              owl:propertyChainAxiom ( hdgi:hasPose
                                       hdgi:involves
                                     ) ;
              rdfs:label "involves" .


###  https://w3id.org/hdgi#isPartOf
hdgi:isPartOf rdf:type owl:ObjectProperty ;
              rdfs:label "isPartOf" .


###  https://w3id.org/hdgi#manufacturedBy
hdgi:manufacturedBy rdf:type owl:ObjectProperty ;
                    rdfs:label "manufacturedBy" .


###  https://w3id.org/hdgi#observes
hdgi:observes rdf:type owl:ObjectProperty ;
              owl:propertyDisjointWith hdgi:performs ;
              rdfs:label "observes" .


###  https://w3id.org/hdgi#performs
hdgi:performs rdf:type owl:ObjectProperty ;
              rdfs:label "performs" .


###  https://w3id.org/hdgi#supportsGesture
hdgi:supportsGesture rdf:type owl:ObjectProperty ;
                     rdfs:label "supportsGesture" .


###  https://w3id.org/hdgi#used
hdgi:used rdf:type owl:ObjectProperty ;
          owl:propertyChainAxiom ( hdgi:hasPose
                                   hdgi:used
                                 ) ;
          rdfs:label "used" .


###  https://w3id.org/hdgi#uses
hdgi:uses rdf:type owl:ObjectProperty ;
          rdfs:label "uses" .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/hdgi#i-component
hdgi:i-component rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:float ;
                 rdfs:comment """In an equation where quaternion (q) = w + xi + yj + zk, where w, x, y, and z are all real numbers and i-component = x 

In the euqation 'i' is defined as sqrt(-1). With quaternions, i = j = k = sqrt(-1).""" ;
                 rdfs:label "i-component" .


###  https://w3id.org/hdgi#j-component
hdgi:j-component rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:float ;
                 rdfs:comment """In an equation where quaternion (q) = w + xi + yj + zk, where w, x, y, and z are all real numbers j-component = y 

In the euqation 'i' is defined as sqrt(-1). With quaternions, i = j = k = sqrt(-1).""" ;
                 rdfs:label "j-component" .


###  https://w3id.org/hdgi#k-component
hdgi:k-component rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:float ;
                 rdfs:comment """In an equation where quaternion (q) = w + xi + yj + zk, where w, x, y, and z are all real numbers k-component = z 

In the euqation 'i' is defined as sqrt(-1). With quaternions, i = j = k = sqrt(-1).""" ;
                 rdfs:label "k-component" .


###  https://w3id.org/hdgi#originPoint
hdgi:originPoint rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:string ;
                 rdfs:comment """Frame of reference or the initial center. 
Location of the (0,0,0) postion related to the given coordinates.""" ;
                 rdfs:label "originPoint" .


###  https://w3id.org/hdgi#timeStamp
hdgi:timeStamp rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:dateTimeStamp ;
               rdfs:label "timeStamp" .


###  https://w3id.org/hdgi#w-component
hdgi:w-component rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:float ;
                 rdfs:comment "Represents the amount of rotation which will occur about this axis defined by (i-component, j-component, k-component). Known as scalar component." ;
                 rdfs:label "w-component" .


###  https://w3id.org/hdgi#x-axisDirection
hdgi:x-axisDirection rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:range [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "leftward" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "rightward" ;
                                                         rdf:rest rdf:nil
                                                       ]
                                            ]
                                ] ;
                     rdfs:label "x-axisDirection" .


###  https://w3id.org/hdgi#xPosition
hdgi:xPosition rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:decimal ;
               rdfs:label "xPosition" .


###  https://w3id.org/hdgi#xRotation
hdgi:xRotation rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:decimal ;
               rdfs:comment "Angle around the x-axis" ;
               rdfs:label "xRotation" .


###  https://w3id.org/hdgi#y-axisDirection
hdgi:y-axisDirection rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:range [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "downward" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "upward" ;
                                                         rdf:rest rdf:nil
                                                       ]
                                            ]
                                ] ;
                     rdfs:label "y-axisDirection" .


###  https://w3id.org/hdgi#yPosition
hdgi:yPosition rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:decimal ;
               rdfs:label "yPosition" .


###  https://w3id.org/hdgi#yRotation
hdgi:yRotation rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:decimal ;
               rdfs:comment "Angle around the y-axis" ;
               rdfs:label "yRotation" .


###  https://w3id.org/hdgi#z-axisDirection
hdgi:z-axisDirection rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:range [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "inward" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "outward" ;
                                                         rdf:rest rdf:nil
                                                       ]
                                            ]
                                ] ;
                     rdfs:label "z-axisDirection" .


###  https://w3id.org/hdgi#zPosition
hdgi:zPosition rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:decimal ;
               rdfs:label "zPosition" .


###  https://w3id.org/hdgi#zRotation
hdgi:zRotation rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:decimal ;
               rdfs:comment "Angle around the z-axis" ;
               rdfs:label "zRotation" .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/hdgi#ActuatableAffordance
hdgi:ActuatableAffordance rdf:type owl:Class ;
                          rdfs:subClassOf hdgi:Affordance .


###  https://w3id.org/hdgi#Affordance
hdgi:Affordance rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty hdgi:affordedBy ;
                                  owl:allValuesFrom hdgi:Device
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty hdgi:supportsGesture ;
                                  owl:allValuesFrom hdgi:Gesture
                                ] ;
                rdfs:comment "Affordances are all the type of properties that belong to devices. They indicate the possible actions users can take with it."@en .


###  https://w3id.org/hdgi#Body
hdgi:Body rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty hdgi:hasPart ;
                            owl:allValuesFrom hdgi:BodyPart
                          ] ;
          rdfs:comment "Human body. A collection of body parts."@en .


###  https://w3id.org/hdgi#BodyPart
hdgi:BodyPart rdf:type owl:Class ;
              rdfs:comment "Parts of human body."@en ;
              skos:example "\"Head\", \"Palm\", \"Finger\" etc." .


###  https://w3id.org/hdgi#Context
hdgi:Context rdf:type owl:Class .


###  https://w3id.org/hdgi#Device
hdgi:Device rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty hdgi:hasContext ;
                              owl:someValuesFrom hdgi:DeviceContext
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty hdgi:manufacturedBy ;
                              owl:allValuesFrom hdgi:DeviceManufacturer
                            ] ;
            rdfs:comment "A device denotes a piece of hardware or software which consists of sensors or actuators or both."@en .


###  https://w3id.org/hdgi#DeviceContext
hdgi:DeviceContext rdf:type owl:Class ;
                   rdfs:subClassOf hdgi:Context .


###  https://w3id.org/hdgi#DeviceManufacturer
hdgi:DeviceManufacturer rdf:type owl:Class ;
                        rdfs:comment "The company manufacturing a device."@en .


###  https://w3id.org/hdgi#Duration
hdgi:Duration rdf:type owl:Class ;
              rdfs:comment "Duration is a temporal extent."@en .


###  https://w3id.org/hdgi#Face
hdgi:Face rdf:type owl:Class ;
          rdfs:subClassOf hdgi:BodyPart ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty hdgi:isPartOf ;
                            owl:someValuesFrom hdgi:BodyPart
                          ] .


###  https://w3id.org/hdgi#FacialGesture
hdgi:FacialGesture rdf:type owl:Class ;
                   rdfs:subClassOf hdgi:Gesture ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty hdgi:involves ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass hdgi:Face
                                   ] .


###  https://w3id.org/hdgi#Finger
hdgi:Finger rdf:type owl:Class ;
            rdfs:subClassOf hdgi:BodyPart ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty hdgi:isPartOf ;
                              owl:someValuesFrom hdgi:BodyPart
                            ] ;
            rdfs:comment "A finger is a body part of the human body and a type of digit."@en ;
            skos:example "thumb, middle finger, left ring finger etc" .


###  https://w3id.org/hdgi#FingerAnticlcokwiseRotation
hdgi:FingerAnticlcokwiseRotation rdf:type owl:Class ;
                                 rdfs:subClassOf hdgi:FingerMovement .


###  https://w3id.org/hdgi#FingerBackward
hdgi:FingerBackward rdf:type owl:Class ;
                    rdfs:subClassOf hdgi:FingerMovement .


###  https://w3id.org/hdgi#FingerClockwiseRotation
hdgi:FingerClockwiseRotation rdf:type owl:Class ;
                             rdfs:subClassOf hdgi:FingerMovement .


###  https://w3id.org/hdgi#FingerDownward
hdgi:FingerDownward rdf:type owl:Class ;
                    rdfs:subClassOf hdgi:FingerMovement .


###  https://w3id.org/hdgi#FingerForward
hdgi:FingerForward rdf:type owl:Class ;
                   rdfs:subClassOf hdgi:FingerMovement .


###  https://w3id.org/hdgi#FingerLeftward
hdgi:FingerLeftward rdf:type owl:Class ;
                    rdfs:subClassOf hdgi:FingerMovement .


###  https://w3id.org/hdgi#FingerMovement
hdgi:FingerMovement rdf:type owl:Class ;
                    rdfs:subClassOf hdgi:Movement ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty hdgi:involves ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass hdgi:Finger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty hdgi:involves ;
                                      owl:maxQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                      owl:onClass hdgi:Finger
                                    ] .


###  https://w3id.org/hdgi#FingerPose
hdgi:FingerPose rdf:type owl:Class ;
                rdfs:subClassOf hdgi:Pose .


###  https://w3id.org/hdgi#FingerRightward
hdgi:FingerRightward rdf:type owl:Class ;
                     rdfs:subClassOf hdgi:FingerMovement .


###  https://w3id.org/hdgi#FingerUpward
hdgi:FingerUpward rdf:type owl:Class ;
                  rdfs:subClassOf hdgi:FingerMovement .


###  https://w3id.org/hdgi#Foot
hdgi:Foot rdf:type owl:Class ;
          rdfs:subClassOf hdgi:BodyPart ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty hdgi:isPartOf ;
                            owl:someValuesFrom hdgi:BodyPart
                          ] .


###  https://w3id.org/hdgi#FootGesture
hdgi:FootGesture rdf:type owl:Class ;
                 rdfs:subClassOf hdgi:Gesture ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hdgi:involves ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass hdgi:Foot
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hdgi:involves ;
                                   owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                   owl:onClass hdgi:Foot
                                 ] .


###  https://w3id.org/hdgi#FootPose
hdgi:FootPose rdf:type owl:Class ;
              rdfs:subClassOf hdgi:Pose .


###  https://w3id.org/hdgi#Forearm
hdgi:Forearm rdf:type owl:Class ;
             rdfs:subClassOf hdgi:BodyPart ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hdgi:isPartOf ;
                               owl:someValuesFrom hdgi:BodyPart
                             ] ;
             rdfs:comment "The forearm is the line between the elbow joint and the wrist."@en .


###  https://w3id.org/hdgi#ForearmAnticlockwiseRotation
hdgi:ForearmAnticlockwiseRotation rdf:type owl:Class ;
                                  rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmBackward
hdgi:ForearmBackward rdf:type owl:Class ;
                     rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmClockwiseRotation
hdgi:ForearmClockwiseRotation rdf:type owl:Class ;
                              rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmDownward
hdgi:ForearmDownward rdf:type owl:Class ;
                     rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmForward
hdgi:ForearmForward rdf:type owl:Class ;
                    rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmGesture
hdgi:ForearmGesture rdf:type owl:Class ;
                    rdfs:subClassOf hdgi:Gesture ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty hdgi:involves ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass hdgi:Forearm
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty hdgi:involves ;
                                      owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                      owl:onClass hdgi:Forearm
                                    ] ;
                    rdfs:comment "Gestures (movements) made with forearm" .


###  https://w3id.org/hdgi#ForearmLeftward
hdgi:ForearmLeftward rdf:type owl:Class ;
                     rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmLeftwardAndDown
hdgi:ForearmLeftwardAndDown rdf:type owl:Class ;
                            rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmLeftwardAndUp
hdgi:ForearmLeftwardAndUp rdf:type owl:Class ;
                          rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmMovement
hdgi:ForearmMovement rdf:type owl:Class ;
                     rdfs:subClassOf hdgi:Movement ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty hdgi:involves ;
                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass hdgi:Forearm
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty hdgi:involves ;
                                       owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                       owl:onClass hdgi:Forearm
                                     ] .


###  https://w3id.org/hdgi#ForearmPose
hdgi:ForearmPose rdf:type owl:Class ;
                 rdfs:subClassOf hdgi:Pose ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hdgi:used ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass hdgi:Forearm
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hdgi:used ;
                                   owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                   owl:onClass hdgi:Forearm
                                 ] .


###  https://w3id.org/hdgi#ForearmRightward
hdgi:ForearmRightward rdf:type owl:Class ;
                      rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmRightwardAndDown
hdgi:ForearmRightwardAndDown rdf:type owl:Class ;
                             rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmRightwardAndUp
hdgi:ForearmRightwardAndUp rdf:type owl:Class ;
                           rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#ForearmUpward
hdgi:ForearmUpward rdf:type owl:Class ;
                   rdfs:subClassOf hdgi:ForearmMovement .


###  https://w3id.org/hdgi#Gesture
hdgi:Gesture rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty hdgi:includesGesture ;
                               owl:allValuesFrom hdgi:Gesture
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hdgi:used ;
                               owl:allValuesFrom hdgi:BodyPart
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty hdgi:hasMovement ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass hdgi:Movement
                             ] ;
             rdfs:comment "A gesture is a movement or a pose of the fingers and/or palm and/or upper arm and/or forearm."@en .


###  https://w3id.org/hdgi#HandGesture
hdgi:HandGesture rdf:type owl:Class ;
                 rdfs:subClassOf hdgi:Gesture ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hdgi:involves ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass hdgi:Finger
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hdgi:involves ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass hdgi:Palm
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hdgi:involves ;
                                   owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                   owl:onClass hdgi:Palm
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hdgi:involves ;
                                   owl:maxQualifiedCardinality "10"^^xsd:nonNegativeInteger ;
                                   owl:onClass hdgi:Finger
                                 ] ;
                 rdfs:comment "Gestures (movements) made with fingers and palms" .


###  https://w3id.org/hdgi#Head
hdgi:Head rdf:type owl:Class ;
          rdfs:subClassOf hdgi:BodyPart ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty hdgi:isPartOf ;
                            owl:someValuesFrom hdgi:BodyPart
                          ] .


###  https://w3id.org/hdgi#HeadGesture
hdgi:HeadGesture rdf:type owl:Class ;
                 rdfs:subClassOf hdgi:Gesture ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty hdgi:involves ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass hdgi:Head
                                 ] .


###  https://w3id.org/hdgi#Human
hdgi:Human rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty hdgi:hasContext ;
                             owl:someValuesFrom hdgi:UserContext
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty hdgi:performs ;
                             owl:someValuesFrom hdgi:Gesture
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty hdgi:performs ;
                             owl:someValuesFrom hdgi:Pose
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty hdgi:uses ;
                             owl:allValuesFrom hdgi:BodyPart
                           ] .


###  https://w3id.org/hdgi#IndexFinger
hdgi:IndexFinger rdf:type owl:Class ;
                 rdfs:subClassOf hdgi:Finger ;
                 rdfs:comment "Second finger or the second digit of hand."@en .


###  https://w3id.org/hdgi#IndexFingerCurled
hdgi:IndexFingerCurled rdf:type owl:Class ;
                       rdfs:subClassOf hdgi:IndexFingerPose .


###  https://w3id.org/hdgi#IndexFingerFolded
hdgi:IndexFingerFolded rdf:type owl:Class ;
                       rdfs:subClassOf hdgi:IndexFingerPose .


